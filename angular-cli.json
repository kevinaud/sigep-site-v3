{
  "project": {
    "version": "1.0.0-beta.18",
    "name": "sigep-site-v3"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "styles.css",
        "theme.scss"
      ],
      "scripts": [
        "./scripts/apiGateway-js-sdk/lib/axios/dist/axios.standalone.js",
        "./scripts/apiGateway-js-sdk/lib/CryptoJS/rollups/hmac-sha256.js",
        "./scripts/apiGateway-js-sdk/lib/CryptoJS/rollups/sha256.js",
        "./scripts/apiGateway-js-sdk/lib/CryptoJS/components/hmac.js",
        "./scripts/apiGateway-js-sdk/lib/CryptoJS/components/enc-base64.js",
        "./scripts/apiGateway-js-sdk/lib/url-template/url-template.js",
        "./scripts/apiGateway-js-sdk/lib/apiGatewayCore/sigV4Client.js",
        "./scripts/apiGateway-js-sdk/lib/apiGatewayCore/apiGatewayClient.js",
        "./scripts/apiGateway-js-sdk/lib/apiGatewayCore/simpleHttpClient.js",
        "./scripts/apiGateway-js-sdk/lib/apiGatewayCore/utils.js",
        "./scripts/apiGateway-js-sdk/apigClient.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
